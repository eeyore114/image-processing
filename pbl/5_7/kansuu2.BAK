#include <stdio.h>
#include <stdlib.h>

void shirokuro(unsigned char f[][256],unsigned char g[][256]){
	int i,j;

	g[][]	 = 255 - f[][];
	
	}



int main()
{
    int             i, j;
    unsigned char   f[256][256];       /** 原画像用の配列 **/
    unsigned char   g[256][256];       /** トリミング後の画像の配列 **/
    FILE            *fp1 ,*fp2;

    /**原画像ファイル読み込み********************************/
	fp1 = fopen("lenna_uchar_256-256.raw" , "rb");
	fread( f , 1 , 256 * 256 , fp1);
	fclose( fp1 );

    /****************************************************************/


    /**↓↓プログラム作成開始↓↓************************************/
	for(i=0;i<256;i++){
		for(j=0;j<256;j++){
				printf("%d\n",f[i][j]);
			g[i][j] = shirokuro(f[i][j],g[i][j]);
			printf("%d\n\n\n",g[i][j]);
		}
	}

    /**↑↑プログラム作成終了↑↑************************************/


    /**トリミング画像ファイル書き込み**************************************/
    fp2 = fopen( "kannsuu2.raw" , "wb" );
    fwrite( g , 1 , 256 * 256  , fp2 );
    fclose( fp2 );
    /****************************************************************/

    return 0;
}

